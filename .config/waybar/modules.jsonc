		]
	},
	"backlight/slider": {
		"min": 20,
		"max": 100,
		"orientation": "horizontal"
	},
	"backlight": {
		"interval": 2,
		"align": 0,
		"rotate": 0,
		"format": "{icon} {percent}%",
		"format-icons": [
			"Óèï", "Óèó", "Óèö", "Û∞Éù", "Û∞Éû", "Û∞Éü", "Û∞É†"
		],
		"tooltip-format": "backlight {percent}%",
		"icon-size": 10,
		"on-scroll-up": "~/.config/waybar/scripts/brightness.sh up",
		"on-scroll-down": "~/.config/waybar/scripts/brightness.sh down",
		"smooth-scrolling-threshold": 1
	},
	"custom/hyprsunset": {
		"format": "{}",
		"signal": 10,
		"interval": "once",
		"return-type": "json",
		"on-click": "/home/rosl/.config/waybar/scripts/hyprsunset.sh toggle",
		"exec": "/home/rosl/.config/waybar/scripts/hyprsunset.sh"
    },
