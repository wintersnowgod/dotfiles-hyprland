{
	"custom/menu": {
		"format": "Û∞£á",
		"on-click": "~/.config/waybar/scripts/wofi.sh",
		"tooltip-format": "Show Wofi Menu"
	},

	// Workspaces
	"hyprland/workspaces": {
		"on-scroll-up": "hyprctl dispatch workspace r-1",
		"on-scroll-down": "hyprctl dispatch workspace r+1",
		"on-click": "activate",
		"active-only": false,
		"all-outputs": true,
		"format": "{}",
		"format-icons": {
		"urgent": "",
		"active": "",
		"default": ""
		},
		"persistent-workspaces": {
		"*": 5
		}
	},

	// Hyprland Window
	"hyprland/window": {
		"max-length": 60,
		"format": "{class}",
		"rewrite": {
		"(.*) - Brave": "$1",
		"(.*) - Chromium": "$1",
		"(.*) - Brave Search": "$1",
		"(.*) - Outlook": "$1",
		"(.*) Microsoft Teams": "$1"
		},
		"separate-outputs": true,
		"icon": true
	},

	// Keyboard Layout
	"hyprland/language": {
		"format": "{short}"
	},

	// System tray
	"tray": {
		"icon-size": 21,
		"spacing": 10
	},

	// Clock
	"clock": {
		"interval": 1,
		"format": "{:%I:%M %p ÔÄó %Z}",
		"format-alt": "{:%A, %B %d, %Y üóì}",
		"tooltip-format": "<tt><small>{calendar}</small></tt>",
		"timezones": [
			"Asia/Shanghai",
			"Asia/Kathmandu"
		],
		"calendar": {
			"mode"          : "month",
			"mode-mon-col"  : 3,
			"weeks-pos"     : "right",
			"on-scroll"     : 1,
			"format": {
				"months":     "<span color='#ffead3'><b>{}</b></span>",
				"days":       "<span color='#ecc6d9'><b>{}</b></span>",
				"weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
				"weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
				"today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
			}
		},
		"actions": {
			"on-scroll-up": "tz_up",
			"on-scroll-down": "tz_down"
		},
		"on-click-right": "~/.config/waybar/scripts/calendar.sh"
	},

	"idle_inhibitor": {
		"format": "{icon}",
		"format-icons": {
			"activated": "ÔÅÆ ",
			"deactivated": "ÔÅ∞ "
		},
		"tooltip-format-activated": "Inhibit Sleep: {status}",
		"tooltip-format-deactivated": "Inhibit Sleep: {status}"
	},

	"bluetooth": {
		"format": "Ôäì",
		"format-on": "Ôäì",
		"format-disabled": "Û∞Ç≥",
		"format-off": "Û∞Ç≥",
		"format-no-controller": "Û∞Ç≥",
		"format-connected": "Û∞Ç± {num_connections}",
		"tooltip-format": "Ôäì {device_alias}",
		"tooltip-format-connected": "{device_enumerate}",
		"tooltip-format-enumerate-connected": "Ôäì {device_alias}",
		"tooltip-format-enumerate-connected-battery": "Ôäì {device_alias}\tÛ∞ÇÑ {device_battery_percentage}%",
		"tooltip-format-on": "No devices Connected",
		"tooltip-format-off": "Bluetooth is turned Off",
		"tooltip-format-disabled":"Bluetooth Controller is Disabled",
		"on-click": "~/.config/waybar/scripts/bluetooth.sh -m",
		"on-click-right": "~/.config/waybar/scripts/bluetooth.sh -t"
	},

	"privacy": {
		"icon-spacing": 4,
		"icon-size": 12,
		"transition-duration": 250,
		"modules": [
			{
				"type": "screenshare",
				"tooltip": true,
				"tooltip-icon-size": 24
			},
			{
				"type": "audio-out",
				"tooltip": true,
				"tooltip-icon-size": 24
			},
			{
				"type": "audio-in",
				"tooltip": true,
				"tooltip-icon-size": 24
			}
		]
	},

	"mpris": {
		"interval": 1,
		"format": "{player_icon}",
		"format-paused": "{status_icon}",
		"on-click": "playerctl play-pause",
		"on-click-right": "pavucontrol -t 1",
		"on-scroll-up": "playerctl previous",
		"on-scroll-down": "playerctl next",
		"scroll-step": 5.0,
		"smooth-scrolling-threshold": 1,
		"player-icons": {
			"chromium": "Ôâ®",
			"brave": "Û±¢º",
			"default": "ÔÄù",
			"firefox": "Ôâ©",
			"kdeconnect": "Ôç≥",
			"mopidy": "ÔÄÅ",
			"mpv": "Û∞êπ",
			"spotify": "ÔÜº",
			"vlc": "Û∞ïº"
		},
		"status-icons": {
			"paused": "‚è∏",
			"playing": "ÔÅã",
			"stopped": "ÔÅç"
		}
	},

// Sysmon
	"group/sysmon": {
	"orientation": "inherit",
	"modules": [
		"cpu",
		"temperature",
		"memory"
		]
	},

	"cpu": {
		"format": "Ôãõ {usage}%",
		"interval": 1,
		"on-click": "~/.config/waybar/scripts/system-monitor.sh"
	},

	"memory": {
		"format": "ÔÉâ {}%",
		"interval": 1,
		"on-click": "~/.config/waybar/scripts/system-monitor.sh"
	},

	"temperature": {
		"interval": 1,
		"critical-threshold": 100,
		"format-critical": "Ôãá {temperatureC}",
		"format": "Ôãâ {temperatureC}¬∞C",
		"on-click": "~/.config/waybar/scripts/system-monitor.sh"
	},

	// Network
	"network": {
		"format": "{ifname}",
		"interval": 1,
		"format-wifi": "{icon}  ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
		"format-ethernet": "Û∞åò ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
		"format-disconnected": "‚ö†",
		"tooltip-format": "Û∞åò {ifname} via {gwaddri}",
		"tooltip-format-wifi": "{icon} {ifname} @ {essid}\nIP: {ipaddr}\nStrength: {signalStrength}%\nFreq: {frequency}MHz\nUp: {bandwidthUpBits} Down: {bandwidthDownBits}",
		"tooltip-format-ethernet": "Ôûñ {ifname}\nIP: {ipaddr}\n up: {bandwidthUpBits} down: {bandwidthDownBits}",
		"tooltip-format-disconnected": "‚ö† Disconnected",
		"max-length": 50,
		"format-icons": [ "Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®" ],
		"on-click": "~/.config/waybar/scripts/networkmanager.sh"
	},

// Audio
	"group/audio": {
		"orientation": "inherit",
		"drawer": {
			"children-class": "audio-child",
			"transition-duration": 500,
			"transition-left-to-right": true
		},
		"modules": [
			"wireplumber#sink",
			"pulseaudio/slider",
			"wireplumber#source"
		]
	},
	"pulseaudio/slider": {
		"min": 0,
		"max": 100,
		"orientation": "horizontal"
	},
    "wireplumber#sink": {
		"node-type": "Audio/Sink",
		"format": "{icon} {volume}%",
		"format-bluetooth": "{icon} {volume}% Ôäî",
		"format-bluetooth-muted": "Û∞ùü Ôäî",
		"format-muted": "Û∞ùü ",
		"format-icons": {
			"headphone": "ÔÄ•",
			"hands-free": "Ôä†",
			"headset": "Óπô",
			"phone": "ÔÇï",
			"portable": "ÔÇï",
			"car": "ÔÜπ",
			"default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
		},
		"max-volume": 100,
		"on-click": "wpctl set-mute @DEFAULT_SINK@ toggle",
		"on-click-right": "pavucontrol -t 3"
	},
	"wireplumber#source": {
		"node-type": "Audio/Source",
		"format": "{icon} {volume}%",
		"format-bluetooth": "{icon} {volume}% Ôäî",
		"format-muted": "ÔÑ± ",
		"format-bluetooth-muted": "ÔÑ± Ôäî",
